            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">// declare variables
def errorMsg = &quot;&quot;;
def expectedResponse = &quot;2001&quot;;

def actualResponse = prev.getResponseCode();

// replaces the HTTP Response Data with the response Code
SampleResult.setResponseData(actualResponse);

// see the response Code in log viewer
log.info(actualResponse);

if(!expectedResponse.equals(actualResponse)){
	errorMsg += &quot;Expected &lt;response code&gt; [&quot; + expectedResponse + &quot;] but we got &lt;response code&gt; [&quot; + actualResponse + &quot;]&quot; ;
}

// see the failure message in the Assertion results
if (errorMsg?.trim()) {
	AssertionResult.setFailureMessage(errorMsg);
     AssertionResult.setFailure(true);    
}</stringProp>
